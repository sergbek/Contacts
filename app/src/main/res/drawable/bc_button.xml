<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#ff5db6ff"/>
            <gradient
                android:angle="270"
                android:startColor="#20DBE8"
                android:endColor="#0D9EFF"
                android:type="linear"/>
            <corners android:radius="10dp"/>

        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff5db6ff"/>
            <gradient
                android:angle="270"
                android:startColor="#05E0E8"
                android:endColor="#E8E0FF"
                android:type="linear"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>

</selector>